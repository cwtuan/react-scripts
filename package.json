  "homepage": "https://github.com/keenai/react-scripts#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/keenai/react-scripts.git"
  },
  "license": "MIT",
  "bin": {
    "react-scripts": "./lib/react-scripts/index.js"
  },
  "scripts": {
    "build": "rm -rf ./lib && babel ./src --out-dir ./lib",
    "start": "yarn build --watch",
    "flow:install-definitions": "flow-typed install -s -o --ignoreDeps peer",
    "flow:remove-definitions": "rm -rf ./flow-typed/npm/*",
    "flow:update-definitions": "yarn flow:remove-definitions && yarn flow:install-definitions"
  },
  "devDependencies": {
    "@keenai/babel-preset-keenai": "^0.6.1",
    "@keenai/eslint-config-keenai": "^0.5.0",
    "babel-cli": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.7.2",
    "flow-bin": "^0.56.0"
  },
  "peerDependencies": {
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react": "^15.5.4"
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "apollo-codegen": "^0.16.5",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "circular-dependency-plugin": "^4.2.1",
    "commander": "^2.11.0",
    "compression-webpack-plugin": "^1.0.0",
    "cross-spawn": "^5.1.0",
    "csv-string": "^2.3.2",
    "detect-port": "^1.1.3",
    "dotenv": "^4.0.0",
    "enzyme": "^3.0.0",
    "express": "^4.16.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.2",
    "graphql-tag": "^2.4.2",
    "graphql": "^0.10.5",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "jest-transform-graphql": "^2.1.0",
    "jest": "^21.0.0",
    "lodash": "^4.17.4",
    "node-uuid": "^1.4.8",
    "react-dev-utils": "^4.1.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
